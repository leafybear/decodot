docker run -d \
		-v /mnt/mixed:/media/mixed \
		-v /mnt/a:/media/a \
		-v /mnt/b:/media/b \
		-v /mnt/c:/media/c \
		-v /mnt/d:/media/d \
		-v /mnt/e:/media/e \
		-e AVAHI=1 --net "host" \
		--name="afp" \
		-e AFP_USER="$(cat ~/sweetie-docker/user.txt)" \
		-e AFP_PASSWORD=$(cat ~/sweetie-docker/passwd.txt) \
		leafybear/afp
